---
// Components
// - Layout
import Layout from '../layouts/Layout.astro'
// - UI
import Hero from '../components/blocks/hero/PageHeader.astro'
import VideoFeed from '../components/blocks/videofeeds/BasicFeed.astro'
import CTA from '../components/blocks/CTA/BasicLight.astro'

// Data
import feedData from '../data/json-files/videoData.json'

// Content
// - SEO

import SEOData from '../data/json-files/seoData.json'

// - Page Header
---

<Layout title={SEOData.tutorials.title} description={SEOData.tutorials.description}>
	<Hero title={SEOData.tutorials.headertitle} text={SEOData.tutorials.headertext} />
	<VideoFeed data={feedData} />
	<CTA />
</Layout>
